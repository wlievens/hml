<?xml version="1.0" encoding="UTF-8"?>
<coatOfArms xmlns="http://github.com/wlievens/heraldry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://github.com/wlievens/heraldry ../src/main/resources/xsd/heraldry.xsd">
    <shape>heater-shield</shape>
    <background>
        <division type="quarterly">
            <part position="1">
                <background>
                    <variation type="barry" firstTincture="azure" secondTincture="vert"/>
                </background>
            </part>
            <part position="2">
                <background>
                    <variation type="paly" firstTincture="argent" secondTincture="purpure"/>
                </background>
            </part>
            <part position="3">
                <background>
                    <variation type="chevronny" firstTincture="or" secondTincture="vert"/>
                </background>
            </part>
            <part position="4">
                <background>
                    <variation type="chevronny-inverted" firstTincture="sable" secondTincture="gules"/>
                </background>
            </part>
        </division>
    </background>
</coatOfArms>

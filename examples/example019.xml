<?xml version="1.0" encoding="UTF-8"?>
<coatOfArms xmlns="http://github.com/wlievens/heraldry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://github.com/wlievens/heraldry ../src/main/resources/xsd/heraldry.xsd">
    <shape>heater-shield</shape>
    <background>
        <division type="quarterly">
            <part position="1">
                <background>
                    <field tincture="argent"/>
                </background>
                <charges>
                    <ordinary type="bend" tincture="azure" line="plain"/>
                </charges>
            </part>
            <part position="2">
                <background>
                    <field tincture="sable"/>
                </background>
                <charges>
                    <ordinary type="bend" tincture="or" line="wavy"/>
                </charges>
            </part>
            <part position="3">
                <background>
                    <field tincture="argent"/>
                </background>
                <charges>
                    <ordinary type="bend" tincture="vert" line="indented"/>
                </charges>
            </part>
            <part position="4">
                <background>
                    <field tincture="sable"/>
                </background>
                <charges>
                    <ordinary type="bend" tincture="gules" line="indented"/>
                </charges>
            </part>
        </division>
    </background>
</coatOfArms>

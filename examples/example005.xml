<?xml version="1.0" encoding="UTF-8"?>
<coatOfArms xmlns="http://github.com/wlievens/heraldry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://github.com/wlievens/heraldry ../src/main/resources/xsd/heraldry.xsd">
    <title>Behnsdorf</title>
    <blazon>Party per pale argent and vert, a tree eradicated counterchanged</blazon>
    <shape>heater-shield</shape>
    <background>
        <division type="pale">
            <part position="1">
                <background>
                    <field tincture="argent"/>
                </background>
            </part>
            <part position="2">
                <background>
                    <field tincture="vert"/>
                </background>
            </part>
        </division>
    </background>
    <charges>
        <mobile figure="tree"/>
    </charges>
</coatOfArms>
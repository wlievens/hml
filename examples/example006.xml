<?xml version="1.0" encoding="UTF-8"?>
<coatOfArms xmlns="http://github.com/wlievens/heraldry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://github.com/wlievens/heraldry ../src/main/resources/xsd/heraldry.xsd">
    <title>John Cary</title>
    <blazon>Argent, on a bend sable three roses of the field</blazon>
    <shape>heater-shield</shape>
    <background>
        <field tincture="argent"/>
    </background>
    <charges>
        <ordinary type="bend" tincture="sable">
            <charges>
                <repeat number="3">
                    <mobile figure="rose" tincture="argent"/>
                </repeat>
            </charges>
        </ordinary>
    </charges>
</coatOfArms>
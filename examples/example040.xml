<?xml version="1.0" encoding="UTF-8"?>
<coatOfArms xmlns="http://github.com/wlievens/heraldry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://github.com/wlievens/heraldry ../src/main/resources/xsd/heraldry.xsd">
    <shape>heater-shield</shape>
    <background>
        <division type="bend-sinister">
            <part position="1">
                <background>
                    <field tincture="or"/>
                </background>
            </part>
            <part position="2">
                <background>
                    <field tincture="vert"/>
                </background>
            </part>
        </division>
    </background>
    <charges>
        <repeat number="2">
            <mobile figure="fleur-de-lis">
                <background>
                    <counterchanged firstTincture="vert" secondTincture="or"/>
                </background>
            </mobile>
        </repeat>
        <ordinary type="bordure">
            <background>
                <counterchanged firstTincture="vert" secondTincture="or"/>
            </background>
        </ordinary>
    </charges>
</coatOfArms>
